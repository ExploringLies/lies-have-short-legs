"\"; function sharethefacts_toggleEmbedBox(e){var embed_box=e.parentElement.parentElement.parentElement.getElementsByClassName('sharethefacts-embed_box')[0]; if(embed_box.style.display=='block'){embed_box.style.display='none';}else{var factbox=e; while (!factbox.classList.contains('sharethefacts')){factbox=factbox.parentElement;}var container=document.createElement(\"div\"); container.appendChild(factbox.cloneNode(true)); embed_box.innerHTML=embed_html; embed_box.style.display='block';}}function sharethefacts_toggleShareVisibility(e){var e=e.parentElement.parentElement.getElementsByClassName('sharethefacts-share')[0]; if (e.style.display==='block'){e.style.display='none';}else{var factbox=document.getElementsByClassName('sharethefacts')[0]; embed_html=factbox.outerHTML; e.style.display='block';}}//adopted from https://jsfiddle.net/stichoza/eyxtj/ function sharethefacts_facebookShare(url, picture, winWidth, winHeight){sharethefacts_openShare('http://www.facebook.com/sharer.php?app_id=612033658951055&u=' + url + '&picture=' + picture, winWidth, winHeight);}function sharethefacts_twitterShare(tweet_id, winWidth, winHeight){sharethefacts_openShare('https://twitter.com/intent/retweet?tweet_id=' + tweet_id, winWidth, winHeight);}function sharethefacts_openShare(url, winWidth, winHeight){var winTop=(screen.height / 2) - (winHeight / 2); var winLeft=(screen.width / 2) - (winWidth / 2); window.open(url, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);}</script> <div class=\"sharethefacts-header\">Share The Facts</div><div class=\"sharethefacts-name\"> <img src=\"http://static.politifact.com.s3.amazonaws.com/politifact%2Fmugs%2FTed_Cruz_1.jpg\"> <div class=\"sharethefacts-speaker\"> <div class=\"sharethefacts-speaker-name\">Ted Cruz</div><div class=\"sharethefacts-speaker-title\">Presidential candidate</div></div></div><div class=\"sharethefacts-rating\"> <img src=\"http://fact-reporter.herokuapp.com/rating-im