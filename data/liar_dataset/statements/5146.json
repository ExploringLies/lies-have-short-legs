/"]}, {"description": "", "id": 219, "photo": null, "resource_uri": "/api/v/2/subject/219/", "subject": "Message Machine 2012", "subject_slug": "message-machine-2012", "used_in_edition": ["/api/v/2/edition/4/", "/api/v/2/edition/5/", "/api/v/2/edition/1/", "/api/v/2/edition/13/", "/api/v/2/edition/7/", "/api/v/2/edition/9/", "/api/v/2/edition/6/", "/api/v/2/edition/2/", "/api/v/2/edition/10/", "/api/v/2/edition/8/"]}, {"description": "", "id": 165, "photo": null, "resource_uri": "/api/v/2/subject/165/", "subject": "Negative Campaigning", "subject_slug": "campaign-advertising", "used_in_edition": ["/api/v/2/edition/5/"]}], "target": [], "twitter_headline": ""}